<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/initbackground"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RequestActivity">

    <ImageView
        android:layout_width="119dp"
        android:layout_height="103dp"
        android:layout_marginStart="118dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/xicara"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="60dp"
        android:layout_height="33dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="150dp"
        android:text="Setor:"
        android:textColor="@color/White"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="59dp"
        android:layout_height="30dp"
        android:layout_marginStart="284dp"
        android:layout_marginTop="128dp"
        android:text=""
        android:textColor="@color/White"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="210dp"
        android:text="Solicitante:"
        android:textColor="@color/White"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="316dp"
        android:layout_marginTop="204dp"
        android:text=""
        android:textColor="@color/White"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="254dp"
        android:layout_height="23dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="300dp"
        android:text="Selecione o que deseja:"
        android:textAlignment="center"
        android:textColor="@color/White"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <CheckBox
        android:id="@+id/cbAgua"
        android:layout_width="149dp"
        android:layout_height="46dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="360dp"
        android:text="Água"
        android:textColor="@color/White"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cbCafe"
        android:layout_width="149dp"
        android:layout_height="46dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="400dp"
        android:text="Café"
        android:textColor="@color/White"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cbCha"
        android:layout_width="149dp"
        android:layout_height="46dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="440dp"
        android:text="Chá"
        android:textColor="@color/White"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cbFruta"
        android:layout_width="149dp"
        android:layout_height="46dp"
        android:layout_marginStart="205dp"
        android:layout_marginTop="360dp"
        android:text="Frutas"
        android:textColor="@color/White"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cbLeite"
        android:layout_width="149dp"
        android:layout_height="46dp"
        android:layout_marginStart="205dp"
        android:layout_marginTop="400dp"
        android:text="Leite"
        android:textColor="@color/White"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cbAcucar"
        android:layout_width="149dp"
        android:layout_height="46dp"
        android:layout_marginStart="205dp"
        android:layout_marginTop="440dp"
        android:text="Açucar"
        android:textColor="@color/White"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btSubmit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="205dp"
        android:layout_marginTop="540dp"
        android:background="@drawable/bts_neutral"
        android:text="Enviar"
        android:textColor="@color/White"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="540dp"
        android:background="@drawable/bts_transparent"
        android:text="Voltar"
        android:textColor="@color/White"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />








</android.support.constraint.ConstraintLayout>